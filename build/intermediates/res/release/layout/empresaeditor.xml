<?xml version="1.0" encoding="utf-8"?>
    <ScrollView android:id="@+id/formaddprod"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    xmlns:android="http://schemas.android.com/apk/res/android">
    <LinearLayout 
    	xmlns:android="http://schemas.android.com/apk/res/android" 
    	android:layout_height="wrap_content" 
    	android:layout_width="fill_parent" 
    	android:id="@+id/linearLayout1" android:orientation="vertical">
    	<TextView android:layout_height="wrap_content" 
    	android:layout_width="wrap_content" 
    	android:text="Editor de Empresa" 
    	android:textAppearance="?android:attr/textAppearanceMedium" 
    	android:id="@+id/textView1"></TextView>
    	<TableLayout 
		    xmlns:android="http://schemas.android.com/apk/res/android"
		    android:layout_height="fill_parent" 
		    android:layout_width="fill_parent"
		    android:background="#000044">
        <TableRow> 
	        <TextView 
	        	android:textAppearance="?android:attr/textAppearanceSmall" 
		        android:text="Rut Empresa" android:layout_height="wrap_content" 
		        android:id="@+id/cod_prodview" android:layout_width="wrap_content">
		    </TextView>
	        <EditText 
	        	android:layout_height="wrap_content" 
		        android:layout_weight="1" 
		        android:id="@+id/editrut_empre" 
		        android:inputType="number"
		        android:layout_width="wrap_content">
	        </EditText>
        </TableRow>
        <TableRow> 
	        <TextView 
	        	android:textAppearance="?android:attr/textAppearanceSmall" 
		        android:text="Nombre Empresa" android:layout_height="wrap_content" 
		        android:id="@+id/descrip_prodview" android:layout_width="wrap_content">
		    </TextView>
	        <EditText 
	        	android:layout_height="wrap_content" 
		        android:layout_weight="1" 
		        android:id="@+id/editnombre_empre" 
		        android:layout_width="wrap_content">
	        </EditText>
        </TableRow> 
        <TableRow> 
	        <TextView 
	        	android:textAppearance="?android:attr/textAppearanceSmall" 
		        android:text="Actividad" android:layout_height="wrap_content" 
		        android:id="@+id/id_actview" android:layout_width="wrap_content">
		    </TextView>
		    <Spinner
	            android:id="@+id/spinneractividad_id_actividad"
	            android:layout_width="wrap_content"
	            android:layout_height="wrap_content" />
         </TableRow> 
        <TableRow> 
	        <TextView 
	        	android:textAppearance="?android:attr/textAppearanceSmall" 
		        android:text="alcance Empresa" android:layout_height="wrap_content" 
		        android:id="@+id/id_subcatview" android:layout_width="wrap_content">
		    </TextView>
	        <EditText 
	        	android:layout_height="wrap_content" 
		        android:layout_weight="1" 
		        android:id="@+id/editalcance_empre" 
		        android:layout_width="wrap_content">
	        </EditText>
         </TableRow> 
        <TableRow> 
	        <TextView 
	        	android:textAppearance="?android:attr/textAppearanceSmall" 
		        android:text="Nombre contacto" android:layout_height="wrap_content" 
		        android:id="@+id/precioview" android:layout_width="wrap_content">
		    </TextView>
	        <EditText 
	        	android:layout_height="wrap_content" 
		        android:layout_weight="1" 
		        android:id="@+id/editnom_cont_empre" 
		        android:layout_width="wrap_content">
	        </EditText>
       </TableRow> 
        <TableRow> 
	        <TextView 
	        	android:textAppearance="?android:attr/textAppearanceSmall" 
		        android:text="Cargo contacto" android:layout_height="wrap_content" 
		        android:id="@+id/marca_prodview" android:layout_width="wrap_content">
		    </TextView>
	        <EditText 
	        	android:layout_height="wrap_content" 
		        android:layout_weight="1" 
		        android:id="@+id/editcargo_cont_empre" 
		        android:layout_width="wrap_content">
	        </EditText>
        </TableRow> 
        <TableRow> 
	        <TextView 
	        	android:textAppearance="?android:attr/textAppearanceSmall" 
		        android:text="Email" android:layout_height="wrap_content" 
		        android:id="@+id/cont_uniview" android:layout_width="wrap_content">
		    </TextView>
	        <EditText 
	        	android:layout_height="wrap_content" 
		        android:layout_weight="1" 
		        android:id="@+id/editemail_empre" 
		        android:inputType="textEmailAddress"
		        android:layout_width="wrap_content">
	        </EditText>
        </TableRow> 
        <TableRow> 
	        <TextView 
	        	android:textAppearance="?android:attr/textAppearanceSmall" 
		        android:text="Telefono" android:layout_height="wrap_content" 
		        android:id="@+id/contenidoview" android:layout_width="wrap_content">
		    </TextView>
	        <EditText 
	        	android:layout_height="wrap_content" 
		        android:layout_weight="1" 
		        android:id="@+id/edittelefono_empre" 
		        android:inputType="number"
		        android:layout_width="wrap_content">
	        </EditText>
        </TableRow> 
    	</TableLayout>
    </LinearLayout>
    </ScrollView>
